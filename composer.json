{
  "name": "configcat/openfeature-provider",
  "description": "ConfigCat OpenFeature Provider for PHP.",
  "license": "MIT",
  "type": "library",
  "keywords": ["feature flag", "feature toggle", "user targeting", "configcat", "openfeature", "provider"],
  "homepage": "https://github.com/configcat/php-openfeature",
  "authors": [
    {
      "name": "ConfigCat",
      "email": "developer@configcat.com",
      "homepage": "https://configcat.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "open-feature/sdk": "^2.0",
    "configcat/configcat-client": "^9.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpstan/phpstan": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "ConfigCat\\OpenFeature\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ConfigCat\\OpenFeature\\Test\\": "tests/"
    }
  }
}
